{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "sourceMap": true,
    "noEmitOnError": true,
    "noEmit": false,
    "composite": false,
    "declaration": true,
    "declarationMap": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "types": [
      "node",
        "jest",
    ],
    "jsx": "react"
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
